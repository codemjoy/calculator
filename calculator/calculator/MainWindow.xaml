        <Button Content="*" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Margin="262,198,0,0" Height="50" Click="Button_Operator"/>
        <Button Content="-" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Margin="262,143,0,0" Height="50" Click="Button_Operator"/>
        <Button Content="+" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Margin="262,88,0,0" Height="50" Click="Button_Operator"/>
        <Button Content="/" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Margin="262,253,0,0" Height="50" Click="Button_Operator"/>
        <Button Content="1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="53" Margin="10,198,0,0" Height="50" Click="Button_Click"/>





    </Grid>
